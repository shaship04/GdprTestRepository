pipeline {
  agent any
    
  tools {nodejs "node"}
    
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/shaship04/GdprTestRepository.git'
      }
    }
        
    stage('Install dependencies') {
      steps {
        sh 'npm install'
        sh 'npm install -S newman-reporter-htmlextra'
      }
    }
     
    stage('Test') {
      steps {
         sh 'npm run testscript'
      }
    }      
  }
}
