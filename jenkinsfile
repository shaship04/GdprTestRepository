pipeline {
  agent any
    
  tools {nodejs "node"}
    
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/shaship04/GdprTestRepository.git'
      }
    }
        
    stage('Install dependencies') {
      steps {
        sh 'npm install'
        sh 'npm install -S newman-reporter-html'
      }
    }
     
    stage('Test') {
      steps {
         sh 'npm run testscript'
         //archive (includes: '**.html')
publishHTML([allowMissing: false, alwaysLinkToLastBuild: false, keepAll: false, reportDir: 'newman', reportFiles: 'HTML_report.html', reportName: 'HTML Report', reportTitles: ''])      }
    }      
  }
}
